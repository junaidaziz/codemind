{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "postinstall": "prisma generate && npm run agent:install",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test && npm run test:e2e",
    "analyze-vercel": "node scripts/analyze-vercel-build.js",
    "fetch-vercel-logs": "node scripts/fetch-vercel-logs.js",
    "fetch-vercel-logs:errors": "node scripts/fetch-vercel-logs.js --status error --limit 5",
    "fetch-vercel-logs:auto-fix": "node scripts/fetch-vercel-logs.js --status error --auto-fix",
    "vercel-webhook:list": "node scripts/configure-vercel-webhook.js --list",
    "vercel-webhook:create": "node scripts/configure-vercel-webhook.js --url",
    "vercel-webhook:delete": "node scripts/configure-vercel-webhook.js --delete",
    "vercel-webhook:test": "node scripts/configure-vercel-webhook.js --test",
    "health:monitor": "node scripts/monitor-deployment-health.js",
    "health:check": "node scripts/monitor-deployment-health.js --timeframe 1h",
    "notifications:setup": "node scripts/setup-notifications.js",
    "notifications:test": "node scripts/setup-notifications.js --test",
    "notifications:verify": "node scripts/test-notifications.js",
    "benchmark:worker-pool": "node scripts/benchmark-worker-pool.mjs",
    "agent:install": "cd services/agent-core && npm install --legacy-peer-deps",
    "agent:build": "cd services/agent-core && npm run build",
    "agent:dev": "cd services/agent-core && npm run dev",
    "agent:start": "cd services/agent-core && npm run start",
    "agent:test": "cd services/agent-core && npm run test",
    "agent:lint": "cd services/agent-core && npm run lint"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@auth/prisma-adapter": "^2.11.0",
    "@heroicons/react": "^2.2.0",
    "@langchain/community": "^0.3.57",
    "@langchain/core": "^0.3.78",
    "@langchain/openai": "^0.6.14",
    "@octokit/auth-app": "^8.1.1",
    "@octokit/rest": "^22.0.0",
    "@prisma/client": "6.17.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@supabase/supabase-js": "^2.75.0",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "fast-xml-parser": "^4.5.0",
    "framer-motion": "^12.23.24",
    "langchain": "^0.3.35",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "next-auth": "5.0.0-beta.29",
    "next-themes": "^0.4.6",
    "openai": "^6.3.0",
    "prisma": "^6.17.1",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "recharts": "^3.2.1",
    "tailwind-merge": "^3.3.1",
    "ts-node": "^10.9.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@babel/parser": "^7.28.4",
    "@babel/traverse": "^7.28.4",
    "@babel/types": "^7.28.4",
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.5.4",
    "@playwright/test": "^1.56.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/babel__traverse": "^7.28.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/winston": "^2.4.4",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "msw": "^2.11.5",
    "playwright": "^1.56.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.5",
    "typescript": "^5",
    "winston": "^3.18.3"
  }
}
